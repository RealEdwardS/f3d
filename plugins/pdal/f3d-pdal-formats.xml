<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/vnd.arrow">
    <comment>Apache Arrow file</comment>
    <glob pattern="*.arrow"/>
  </mime-type>
  <mime-type type="application/octet-stream">
    <comment>Binary Point Format file</comment>
    <glob pattern="*.bpf"/>
    <comment>ILVIS2 File</comment>
    <glob pattern="*.ilvis2"/>
    <comment>Matterport E57 File</comment>
    <glob pattern="*.e57"/>
    <comment>FastBinary format File</comment>
    <glob pattern="*.fbi"/>
    <comment>Sonar Bathymetry Data</comment>
    <glob pattern="*.mbio"/>
    <comment>Python Numpy File</comment>
    <glob pattern="*.npy"/>
    <comment>Corrected Sensor Data files</comment>
    <glob pattern="*.csd"/>
    <comment>Point Cloud Data (PCD) formatted files</comment>
    <glob pattern="*.pcd"/>
    <comment>Polygon file</comment>
    <glob pattern="*.ply"/>
    <comment>QFIT format file</comment>
    <glob pattern="*.qfit"/>
    <comment>RDB file</comment>
    <glob pattern="*.rdb"/>
    <comment>RXP file</comment>
    <glob pattern="*.rxp"/>
    <comment>SBET file</comment>
    <glob pattern="*.sbet"/>
    <comment>Terrasolid file (as a bin file)</comment>
    <glob pattern="*.bin"/>
    <comment>TileDB 2.3.0+</comment>
    <glob pattern="*.tiledb"/>
  </mime-type>
  <mime-type type="application/vnd.las">
    <comment>Cloud Optimized Point Cloud (Comparessed as a LASzip file)</comment>
    <glob pattern="*.las"/>
  </mime-type>
  <mime-type type="model/draco">
    <comment>Google Draco file</comment>
    <glob pattern="*.drc"/>
  </mime-type>
  <mime-type type="application/json">
    <comment>Entwine Point Tile (EPT) as JSON file</comment>
    <glob pattern="*.ept.json"/>
    <comment>Spatio Temporal Access Catalog (STAC) as JSON file</comment>
    <comment>Indexed 3d Scene Layer (I3S) as JSON file</comment>
    <glob pattern="*.json"/>
  </mime-type>
  <mime-type type="application/x-hdf">
    <comment>HDF5 File</comment>
    <glob pattern="*.hdf"/>
    <glob pattern="*.h5"/>
  </mime-type>
  <mime-type type="application/x-matlab">
    <comment>MatLab File</comment>
    <glob pattern="*.mat"/>
  </mime-type>
  <mime-type type="application/vnd.nitf">
    <comment>National Imagery Transmission Format</comment>
    <glob pattern="*.ntif"/>
  </mime-type>
  <mime-type type="text/plain">
    <comment>OBJ format File</comment>
    <glob pattern="*.obj"/>
    <comment>Leica Cyclone PTS file</comment>
    <glob pattern="*.pts"/>
    <comment>Leica Cyclone PTX file</comment>
    <glob pattern="*.ptx"/>
    <comment>Text file</comment>
    <glob pattern="*.txt"/>
  </mime-type>
  <mime-type type="application/vnd.splk">
    <comment>SPLK file</comment>
    <glob pattern="*.splk"/>
  </mime-type>
  <mime-type type="application/vnd.spz">
    <comment>SPZ file</comment>
    <glob pattern="*.spz"/>
  </mime-type>
</mime-info>
